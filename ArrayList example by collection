import java.util.ArrayList;

public class collectionlist {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ArrayList l=new ArrayList();
		l.add("Shivam");
		l.add("A");
		l.add(null);
		l.add("B");
		l.add(20);
		l.add(20);
		System.out.println(l);
		l.set(0, "XX");
		System.out.println(l);
	}

}
